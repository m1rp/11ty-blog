{% extends "layouts/base.njk" %}
{% block content%}
  <article>
    <section class="intro">
      <h1>{{ title }}</h1>
      {% if introduction %}
      <h2 > {{introduction}} </h2> 
      {% endif %}
      <i>{{ date}}</i>
    </section>

    <section class="content" id="content">
    {{ content | safe}}
    </section>

  </article>
{% endblock %}